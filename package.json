    "lint": "./node_modules/jscs/bin/jscs mapd-crossfilter.js",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "gulp": "3.9.1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "1.5.3",
    "istanbul": "0.4.3",
    "jscs": "2.11.0",
    "mocha": "2.4.5",
    "npm-watch": "0.1.3"
  }
}
