{
  "name": "@mapd/mapd-crossfilter",
  "version": "1.1.0",
  "officialVersion": "1.3.12",
  "description": "Fast multidimensional filtering for coordinated views.",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/map-d/mapd-crossfilter.git"
  },
  "watch": {
    "build": {
      "patterns": [
        "mapd-crossfilter"
      ],
      "extensions": "js",
      "ignore": ""
    }
  },
  "scripts": {
    "test": "istanbul cover _mocha -- --compilers js:babel-register test/",
    "coverage": "npm test ; open coverage/lcov-report/index.html",
    "babel": "babel src --out-dir build",
    "webpack": "webpack",
    "build": "bash scripts/build.sh",
    "lint": "jscs src/**/*.js",
    "watch": "npm-watch",
    "clean": "bash scripts/clean.sh",
    "postinstall": "npm install moment"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-spies": "0.7.1",
    "gulp": "3.9.1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "1.5.3",
    "istanbul": "1.0.0-alpha.2",
    "jscs": "2.11.0",
    "mocha": "2.4.5",
    "npm-watch": "0.1.3",
    "webpack": "1.13.1"
  },
  "peerDependencies": {
    "moment": "^2.0.0"
  }
}
